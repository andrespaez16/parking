<%= form_for @vehiculo, :html =>{:class => "form"}, remote: true do |f| %>
    <div class="form-group">
      <%= f.text_field :placa, class:"form-control"  %>
    </div>
    <div class="form-group">
      <%= f.text_field :tarifa, class:"form-control" %>
    </div>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>
  